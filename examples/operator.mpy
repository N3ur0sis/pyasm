
def factorial(n):
    if n == 0 or n == 1:
        return 1
    else:
        return n * factorial(n - 1)

def max_value(a, b, c):
    if a >= b and a >= c:
        return a
    else: # attention ! elif n'est pas dans l'Ã©noncÃ©
        if b >= a and b >= c:
            return b
        else:
            return c

def divide_and_compare(x, y):
    quotient = x // y
    if quotient != 0:
        return x == y
    else:
        return x > y

