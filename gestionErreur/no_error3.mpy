# Programme long correct

def factorial(n):
    if n == 0:
        return 1
    else:
        return n * factorial(n-1)

def fibonacci(n):
    if n <= 1:
        return n
    else:
        return fibonacci(n - 1)+fibonacci(n-  2)

def main():
    print("Factorial:")
    for i in [1,2,  3,4,   5]:
        print("Factorial of", i, "is", factorial(i))
    
    print("Fibon")
    for i in [1,2,3,4,5]:
        print("Fibon of", i, "is", fibonacci(i))

main()