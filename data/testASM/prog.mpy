def add(x, y):
    return x + y

def factorial(n):
    if n <= 1:
        return 1
    return n * factorial(n-1)

def fibonacci(n):
    if n <= 1:
        return n
    return fibonacci(n-1) + fibonacci(n-2)
# ===== Multiple parameters =====
def sum_all(a, b, c, d, e):
    return a + b + c + d + e
# ===== Function returning list =====
def generate_sequence():
    return [10, 20, 30, 40]

