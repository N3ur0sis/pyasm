def sel_sort(Liste):
    n = len(Liste)
    L = list(range(n))
    k = 0
    for e in Liste:
        L[k] = e
        k = k+1
    for i in range(n):
        min_i = i
        for j in range(n):        # single-arg range
            if j > i and L[j] < L[min_i]:
                min_i = j
        tmp = L[i]
        L[i] = L[min_i]
        L[min_i] = tmp
    return L   


unsorted = [9, 3, 1, 6, 4, 2]
print("unsorted         =", unsorted)
print("sorted           =", sel_sort(unsorted))

