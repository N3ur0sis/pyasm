def add(a, b):
    return a + b

def fact(n):
    if n < 2:
        return 1
    return n * fact(n - 1)

def fib(n):
    if n < 2:
        return n
    return fib(n-1) + fib(n-2)

def mixed(n):
    s = 0
    for i in range(n):
        s = add(s, i)
    if s > 10 and n != 4:
        print("big")
    return s

